{
  "name": "@daomu/flexi-ui",
  "private": false,
  "version": "0.0.7",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./Button": {
      "types": "./dist/types/compoents/Button/Button.d.ts",
      "import": "./dist/esm/compoents/Button/Button.js"
    },
    "./DataTable": {
      "types": "./dist/types/compoents/DataTable/DataTable.d.ts",
      "import": "./dist/esm/compoents/DataTable/DataTable.js"
    },
    "./FileUploader": {
      "types": "./dist/types/compoents/FileUploader/FileUploader.d.ts",
      "import": "./dist/esm/compoents/FileUploader/FileUploader.js"
    },
    "./VirtualList": {
      "types": "./dist/types/compoents/VirtualList/VirtualList.d.ts",
      "import": "./dist/esm/compoents/VirtualList/VirtualList.js"
    },
    "./hooks/useLocalStorage": {
      "types": "./dist/types/hooks/useLocalStorage.d.ts",
      "import": "./dist/esm/hooks/useLocalStorage.js"
    },
    "./hooks/useTheme": {
      "types": "./dist/types/hooks/useTheme.d.ts",
      "import": "./dist/esm/hooks/useTheme.js"
    },
    "./hooks/useForm": {
      "types": "./dist/types/hooks/form/useFormRefactored.d.ts",
      "import": "./dist/esm/hooks/form/useFormRefactored.js"
    },
    "./theme/tokens": {
      "types": "./dist/types/theme/tokens.d.ts",
      "import": "./dist/esm/theme/tokens.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "rollup -c && pnpm run build:types",
    "build:types": "tsc -p tsconfig.lib.json",
    "prepublishOnly": "pnpm run build:lib",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:dev": "vitepress dev docs --port 3000",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 4173",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.2.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "framer-motion": "^12.23.12",
    "lodash-es": "^4.17.21",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-a11y": "9.1.1",
    "@storybook/addon-docs": "9.1.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.1.1",
    "@storybook/addon-onboarding": "9.1.1",
    "@storybook/addon-vitest": "9.1.1",
    "@storybook/blocks": "^8.6.14",
    "@storybook/builder-vite": "^9.1.1",
    "@storybook/react": "^9.1.1",
    "@storybook/react-vite": "9.1.1",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "9.1.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^9.1.1",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vitepress": "^1.0.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.12"
  }
}
